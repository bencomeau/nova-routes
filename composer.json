{
    "name": "bencomeau/nova-routes",
    "description": "Tool for managing application routes within the Laravel Nova dashboard.",
    "keywords": [
        "laravel",
        "nova",
        "routes"
    ],
    "homepage": "https://github.com/bencomeau/nova-routes",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "BenComeau\\NovaRoutes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BenComeau\\NovaRoutes\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
